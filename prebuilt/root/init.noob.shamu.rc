on property:sys.boot_completed=1
   stop mpdecision

   # Default governor - interactiveX
   chown system system /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor
   chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor
   write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor interactiveX
   chown system system /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor
   chmod 0664 /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor
   write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor interactiveX
   chown system system /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor
   chmod 0664 /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor
   write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor interactiveX
   chown system system /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor
   chmod 0664 /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor
   write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor interactiveX
   
   # Default min - 300 MHz
   chown system system /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
   chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
   write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 300000
   chown system system /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 
   chmod 0664 /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq
   write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 300000
   chown system system /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq
   chmod 0664 /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq
   write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 300000
   chown system system /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq
   chmod 0664 /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq
   write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq 300000
   
   # Default max - 2649 MHz
   chown system system /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq
   chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq
   write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 2649600
   chown system system /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq
   chmod 0664 /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq
   write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 2649600
   chown system system /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq
   chmod 0664 /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq
   write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 2649600
   chown system system /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq
   chmod 0664 /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq
   write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 2649600
   
   # Input boost settings
   write /sys/module/cpu_boost/parameters/input_boost_freq 0
   write /sys/module/cpu_boost/parameters/input_boost_ms 0

   # Default I/O values
   # 512 sounds good. "Fiops" set as default i/o scheduler in defconfig.
   write /sys/block/mmcblk0/queue/read_ahead_kb 512
   
   # Default I/O scheduler - deadline
   write /sys/block/mmcblk0/queue/scheduler deadline
   
   # Wakelock
   write /sys/module/xhci_hcd/parameters/wl_divide 17
   
   # Powersuspend
   write /sys/kernel/power_suspend/power_suspend_mode 1
   write /sys/kernel/power_suspend/power_suspend_state 1
   
   # Lowmemorykiller
   write /sys/module/lowmemorykiller/parameters/enable_adaptive_lmk 1
   
   # Thermal driver
   write /sys/module/msm_thermal/parameters/enabled Y
   write /sys/module/msm_thermal/core_control/enabled 0
   write /sys/module/msm_thermal/vdd_restriction/enabled 0
   
   # Gpu Settings
   write /sys/module/simple_gpu_algorithm/parameters/simple_gpu_activate 1
   write /sys/module/simple_gpu_algorithm/parameters/simple_laziness 2
   write /sys/module/simple_gpu_algorithm/parameters/simple_ramp_threshold 8000
   write /sys/module/adreno_idler/parameters/adreno_idler_idlewait 44
   write /sys/module/adreno_idler/parameters/adreno_idler_idleworkload 3000
   write /sys/class/kgsl/kgsl-3d0/max_gpuclk 600000000
   
   # Default vibration sensitivity. Stock Value is 127 = 100%. That's fucking obnoxious if you ask me?
   # Range of acceptable values is 0-127. Let's use "44 = 35%" since it's much more subtle.
   write /sys/vibrator/pwmvalue 44
